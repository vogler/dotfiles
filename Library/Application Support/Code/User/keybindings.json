// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "alt+k",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "alt+j",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "shift+alt+k",
        "command": "workbench.action.moveEditorRightInGroup"
    },
    {
        "key": "shift+alt+j",
        "command": "workbench.action.moveEditorLeftInGroup"
    },
    {
        "key": "cmd+=",
        "command": "workbench.action.zoomReset"
    },
    {
        "key": "cmd+m cmd+d",
        "command": "git.openChange",
        "when": "!textCompareEditorVisible"
    },
    {
        "key": "cmd+m cmd+d",
        "command": "git.openFile",
        "when": "textCompareEditorVisible"
    },
    {
      "key": "cmd+m cmd+m",
      "command": "toggle.diff.editorMode"
    },
    {
        "key": "cmd+m cmd+j",
        "command": "workbench.action.compareEditor.nextChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "cmd+m cmd+k",
        "command": "workbench.action.compareEditor.previousChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "cmd+m cmd+j",
        "command": "workbench.action.editor.nextChange",
        "when": "!textCompareEditorVisible"
    },
    {
        "key": "cmd+m cmd+k",
        "command": "workbench.action.editor.previousChange",
        "when": "!textCompareEditorVisible"
    },
    {
        "key": "shift+cmd+j",
        "command": "workbench.action.focusPanel",
        "when": "editorFocus"
    },
    {
        "key": "shift+cmd+j",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!editorFocus"
    },
    {
      "key": "cmd+m cmd+s",
      "command": "git.stageSelectedRanges"
    },
    { "key": "shift+cmd+=",         "command": "editor.action.fontZoomIn" },
    { "key": "cmd+=",               "command": "editor.action.fontZoomIn" },
    { "key": "shift+cmd+-",         "command": "editor.action.fontZoomOut" },
    { "key": "cmd+-",               "command": "editor.action.fontZoomOut" },
    { "key": "cmd+0",               "command": "editor.action.fontZoomReset" },
  ]