# Added by OrbStack: 'orb' SSH host for Linux machines
# This only works if it's at the top of ssh_config (before any Host blocks).
# This won't be added again if you remove it.
Include ~/.orbstack/ssh/config

Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519

# -------------------------------

Host rpi3
  HostName rpi3.voglerr.de
  User pi

# ipv6 would work via port 22 as well, but for ipv4 access we need port forwarding
Host rpi4
  HostName rpi4.voglerr.de
  User pi
  Port 24

Host vps
  HostName oracle.voglerr.de

# futro-s920
Host edi
  HostName edi.voglerr.de

# -------------------------------

Host bf
  HostName bitflow-dev.de
  IdentityFile ~/.ssh/oracle-bitflow-2023-02-15.key
